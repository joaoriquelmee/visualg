algoritmo "VetoresParess"
var val: vetor[1..7] de inteiro
cont, quantp: inteiro

inicio
      Para cont <- 1 ate 7 faca
           Escreva("Digite o numero do", cont, "o. vetor :")
           Leia(val[cont])
             Se(val%2 = 0) entao
             quantp <-  1
             FimSe
      FimPara
fimalgoritmo
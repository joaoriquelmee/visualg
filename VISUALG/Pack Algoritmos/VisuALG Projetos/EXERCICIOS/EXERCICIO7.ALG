algoritmo "Exercicio7"
var num, dobro, terc: real

inicio
      Escreva("Digite um número : ")
      Leia (num)
      dobro <- (num * 2)
      terc <- (num / 3)
      Escreval("-----------------------------------")
      Escreval("O dobro de ", num, " é =", dobro)
      Escreval("A terça parte de ", num, " é =", terc)
fimalgoritmo